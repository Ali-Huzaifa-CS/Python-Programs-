   ],
   "source": [
    "number = [5,10,9,88,150,0,3,9]\n",
    "sum = 0\n",
    "for i in range(len(number)):\n",
    "    sum = sum + number[i]\n",
    "\n",
    "print(\"The total of number in list is : \",sum)\n"
   ]
